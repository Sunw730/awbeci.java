<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.awbeci.mapper.UserCategoryMapper">
    <resultMap id="UserCategoryResult" type="UserCategory">
        <id property="id" column="id"/>
        <collection property="userSiteses" resultMap="UserSitesResult"/>
    </resultMap>

    <resultMap id="UserSitesResult" type="UserSites">
        <id property="id" column="id"/>
        <result property="name" column="name"/>
        <result property="url" column="url"/>
        <result property="icon" column="icon"/>
        <result property="sortNo" column="sortNo"/>
    </resultMap>
</mapper>